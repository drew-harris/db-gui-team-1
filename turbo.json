{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "dist/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "package.json"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "serve": {
      "dependsOn": ["build"]
    }
  }
}
